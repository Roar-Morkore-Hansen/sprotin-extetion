{
  "manifest_version": 2,
  "name": "Sprotin Extension",
  "description": "Sprotin, n√∫ sum Google Chrome extension",
  "version": "1.4",
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content_scripts.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Sprotin Extension",
    "default_icon":"img/icon.png"
  }
}
